{
	"devDependencies": {
		"@nuxtjs/tailwindcss": "^5.1.3",
		"env-cmd": "^10.1.0",
		"nuxt": "^3.0.0-rc.4"
	},
	"private": true,
	"scripts": {
		"build-development": "env-cmd -e root,development nuxt build",
		"build-release": "env-cmd -e root,release nuxt build",
		"dev": "nuxt dev --https",
		"dev-development": "env-cmd -e root,development yarn run dev",
		"dev-release": "env-cmd -e root,release yarn run dev",
